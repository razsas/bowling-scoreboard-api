{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CorsSettings": {
    "AllowedOrigins": [ "http://localhost:4200" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
    "AllowedHeaders": [
      "Content-Type",
      "Authorization",
      "Accept",
      "Sec-Ch-Ua",
      "Sec-Ch-Ua-Mobile",
      "Sec-Ch-Ua-Platform"
    ]
  },
  "AllowedHosts": "*",
  "TokenKey": "super secret key that needs to be long enough otherwise the unhandled exception will kill us",
